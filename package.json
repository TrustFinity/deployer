    "cropper": "^3.0.0",
    "cross-env": "^3.2.3",
    "eslint": "^4.8.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "fontawesome": "^4.7.2",
    "html5shiv": "^3.7.3",
    "ionicons": "^3.0.0",
